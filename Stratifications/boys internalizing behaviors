       mlab=mlabfun7_9("RE Model", resintadj7_9),xlim=c(-40,30),cex=1.2, cex.lab=1,
       ylim=c(-1, 8),
       psize=1, header="Cohort study (N= Attended ECEC, %)", slab=c('ALSPAC (N=88,6.46% )', 'GenR (N=428, 66.05%)','DNBC (N=3692, 25.10%)', 'INMA (N=67, 30.45%)', 'EDEN (N=361, 81.49%)'))

### add column headings to the plot
text(15,7,cex=1.2,font=2, c("Weights (%)"))






