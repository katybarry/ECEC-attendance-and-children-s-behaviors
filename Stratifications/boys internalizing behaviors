boys.int.adj <- tibble(
  age_bracket=c('5-6 years (N= 2327,31.20%)',
                '7-9 years (N= 4636, 26.70%)'),
  estimates=c(-1.11, -1.68),
  low_ci=c(-2.65, -2.81),
  high_ci=c(0.43,-0.56),
  p_value=c(0.16,0.003),
  studies=c('4','5'),
  heterogeneity=c(
    'Q=3.23, df=3, p=0.36; I^2=0.0%, H^2=1.00',
    'Q=1.96, df=4, p=0.74; I^2=0.0%, H^2=1.00'))

