  ci.lb = boys.int.adj  %>% pull(low_ci),
  ci.ub = boys.int.adj %>% pull(high_ci),
  slab = boys.int.adj %>% pull(age_bracket),
  xlab = "Beta Coefficient (95% CI)",
  ilab =  cbind(
    boys.int.adj  %>% pull(p_value),
    boys.int.adj %>% pull(heterogeneity),
    boys.int.adj  %>% pull(studies)),
  textpos = c(age_pos, est_pos),
  ilab.xpos = c(p_pos, het_pos, st_pos),
  ilab.pos = 4,
  cex = 0.8,
  cex.axis = 0.8,
  refline = 0,
  xlim = c(-36.0, 28),
  ylim = c(0, height),
  alim = c(-10, 10),
  steps = 3,
  digits = c(2, 2),
  psize = 1.2,
  font = "arial")

