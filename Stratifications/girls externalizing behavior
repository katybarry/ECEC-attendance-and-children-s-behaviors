                (ext_5_6_adj_genr$coefficients["f.centre_care.5_71", "Std. Error"]),
                (ext_5_6_adj_eden$coefficients["f.centre_care.5_71", "Std. Error"]),
                (ext_5_6_adj_elfe$coefficients["f.centre_care.5_71", "Std. Error"]))

