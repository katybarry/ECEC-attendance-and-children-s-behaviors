girls.ext.crude <- tibble(
  age_bracket=c('5-6 years (N= 2260,31.80%)',
                '7-9 years (N= 4303, 26.40%)'),
  estimates=c(-1.49, -1.97),
  low_ci=c(-2.97, -3.04),
  high_ci=c(-0.01,-0.91),
  p_value=c(0.05,0.003),
  studies=c('4','5'),
  heterogeneity=c(
    'Q=2.09, df=3, p=0.55; I^2=0.0%, H^2=1.00',
    'Q=4.85, df=4, p=0.30; I^2=0.0%, H^2=1.00'))

png(
  file = "externalizing crude girls ECEC attendance.png",
  width = 50,
  height = 12,
  units = "cm",
  res = 300)
par(mar=c(5,0,0,0))
age_pos = -36
est_pos = -20
p_pos = -19
het_pos = 17
st_pos = 15
height = 5

forest(
  x = girls.ext.crude %>% pull(estimates),
  ci.lb = girls.ext.crude  %>% pull(low_ci),
  ci.ub = girls.ext.crude %>% pull(high_ci),
  slab = girls.ext.crude %>% pull(age_bracket),
  xlab = "Beta Coefficient (95% CI)",
  ilab =  cbind(
    girls.ext.crude  %>% pull(p_value),
    girls.ext.crude %>% pull(heterogeneity),
    girls.ext.crude  %>% pull(studies)),
  textpos = c(age_pos, est_pos),
  ilab.xpos = c(p_pos, het_pos, st_pos),
  ilab.pos = 4,
  cex = 0.8,
  cex.axis = 0.8,
  refline = 0,
  xlim = c(-36.0, 28),
  ylim = c(0, height),
  alim = c(-10, 10),
  steps = 3,
  digits = c(2, 2),
  psize = 1.2,
  font = "arial")

text(age_pos, height-1, "Age bracket (N attended ECEC, %)",font=2, cex = 0.8, pos = 4)
text(est_pos, height-1, "Estimate [95% CI]", font=2, cex = 0.8, pos = 2)
text(p_pos, height-1, "P-value",font=2, cex = 0.8, pos = 4)
text(st_pos-2, height-1, "Number of studies",font=2, cex = 0.8, pos = 4)
text(het_pos, height-1, "Heterogeneity",font=2, cex = 0.8, pos = 4)
dev.off()

# ECEC attendance and externalizing behaviors adjusted results ______________________________


resextadj5_6
resextadj7_9

girls.ext.adj <- tibble(
  age_bracket=c('5-6 years (N= 2260,31.80%)',
                '7-9 years (N= 4303, 26.40%)'),
  estimates=c(0.64, -1.26),
  low_ci=c(-2.22, -2.34),
  high_ci=c(3.50,-0.18),
  p_value=c(0.66,0.02),
  studies=c('4','5'),
  heterogeneity=c(
    'Q=5.59, df=3, p=0.13; I^2=40.5%, H^2=1.68',
    'Q=4.88, df=4, p=0.30; I^2=0.0%, H^2=1.00'))

