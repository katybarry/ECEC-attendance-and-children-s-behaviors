resintadj.9

girls.int.adj <- tibble(
  age_bracket=c('5-6 years (N= 2268,31.50%)',
                '7-9 years (N= 4322,26.20%)'),
  estimates=c(-0.24, -1.55),
  low_ci=c(-1.79, -4.98),
  high_ci=c(1.31,1.87),
  p_value=c(0.76,0.37),
  studies=c('4','5'),
  heterogeneity=c(
    'Q=1.67, df=3, p=0.64; I^2=0.0%, H^2=1.00',
    'Q=8.84, df=4, p=0.07; I^2=59.3%, H^2=2.46'))

