resint.9
library('metafor')

mlabfun.9 <- function(text, resint.9) {
  list(bquote(paste(.(text),
                    ": Q = ", .(formatC(resint.9$QE, digits=2, format="f")),
                    ", df = ", .(resint.9$k - resint.9$p),
                    ", p ", .(metafor:::.pval(resint.9$QEp, digits=2, showeq=TRUE, sep=" ")), "; ",
                    I^2, " = ", .(formatC(resint.9$I2, digits=1, format="f")), "%, ",
                    tau^2, " = ", .(formatC(resint.9$tau2, digits=2, format="f")),";",
                    H^2, " = ", .(formatC(resint.9$H2, digits=2, format="f")))))}

forest(resint.9,  showweights=TRUE,
       mlab=mlabfun.9("RE Model", resint.9),xlim=c(-40,30),cex=1.2, cex.lab=1, 
       ylim=c(-1, 9),
       psize=1, header="Cohort study (N= Attended ECEC, %)", slab=c('ALSPAC (N=92,6.19% )', 'GenR (N=429, 65.70%)', 'DNBC (N=3387 (24.69%)', 'EDEN (N=83, 36.10%)', 'INMA (N=331, 81.93%)'))

### add column headings to the plot
text(15,8,cex=1.2,font=2, c("Weights (%)"))


