# ECEC attendance and internalizing behaviors crude results ____________________________________
resint5_6
resint.9

girls.int.crude <- tibble(
  age_bracket=c('5-6 years (N= 2268,31.50%)',
                '7-9 years (N= 4322,26.20%)'),
  estimates=c(-0.56, -2.00),
  low_ci=c(-2.10, -5.75),
  high_ci=c(0.98,1.74),
  p_value=c(0.48,0.29),
  studies=c('4','5'),
  heterogeneity=c(
    'Q=0.13, df=3, p=0.99; I^2=0.0%, H^2=1.00',
    'Q=10.15, df=4, p=0.04; I^2=69.4%, H^2=3.27'))

png(
  file = "internalizing crude girls ECEC attendance.png",
  width = 50,
  height = 12,
  units = "cm",
  res = 300)
par(mar=c(5,0,0,0))
age_pos = -36
est_pos = -20
p_pos = -19
het_pos = 17
st_pos = 15
height = 5

forest(
  x = girls.int.crude %>% pull(estimates),
  ci.lb = girls.int.crude  %>% pull(low_ci),
  ci.ub = girls.int.crude %>% pull(high_ci),
  slab = girls.int.crude %>% pull(age_bracket),
  xlab = "Beta Coefficient (95% CI)",
  ilab =  cbind(
    girls.int.crude  %>% pull(p_value),
    girls.int.crude %>% pull(heterogeneity),
    girls.int.crude  %>% pull(studies)),
  textpos = c(age_pos, est_pos),
  ilab.xpos = c(p_pos, het_pos, st_pos),
  ilab.pos = 4,
  cex = 0.8,
  cex.axis = 0.8,
  refline = 0,
  xlim = c(-36.0, 28),
  ylim = c(0, height),
  alim = c(-10, 10),
  steps = 3,
  digits = c(2, 2),
  psize = 1.2,
  font = "arial")

text(age_pos, height-1, "Age bracket (N attended ECEC, %)",font=2, cex = 0.8, pos = 4)
text(est_pos, height-1, "Estimate [95% CI]", font=2, cex = 0.8, pos = 2)
text(p_pos, height-1, "P-value",font=2, cex = 0.8, pos = 4)
text(st_pos-2, height-1, "Number of studies",font=2, cex = 0.8, pos = 4)
text(het_pos, height-1, "Heterogeneity",font=2, cex = 0.8, pos = 4)
dev.off()

