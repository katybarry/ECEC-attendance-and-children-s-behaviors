#crude forest plot meta-analysis for age bracket 7-9 years__________________________________________


yint7_9 <-c((int_7_9_crude_alspac$coefficients["f.type_care.7_102", "Estimate"]),
            (int_7_9_crude_genr$coefficients["f.type_care.7_102", "Estimate"]),
            (int_7_9_crude_dnbc$coefficients["f.type_care.7_102", "Estimate"]),
            (int_7_9_crude_eden$coefficients["f.type_care.7_102", "Estimate"]))

yint7_9

seint7_9 <-c((int_7_9_crude_alspac$coefficients["f.type_care.7_102", "Std. Error"]), 
             (int_7_9_crude_genr$coefficients["f.type_care.7_102", "Std. Error"]),
             (int_7_9_crude_dnbc$coefficients["f.type_care.7_102", "Std. Error"]),
             (int_7_9_crude_eden$coefficients["f.type_care.7_102", "Std. Error"]))


resint7_9 <- metafor::rma(yint7_9, sei=seint7_9)
resint7_9
library('metafor')

mlabfun7_9 <- function(text, resint7_9) {
  list(bquote(paste(.(text),
                    ": Q = ", .(formatC(resint7_9$QE, digits=2, format="f")),
                    ", df = ", .(resint7_9$k - resint7_9$p),
                    ", p ", .(metafor:::.pval(resint7_9$QEp, digits=2, showeq=TRUE, sep=" ")), "; ",
                    I^2, " = ", .(formatC(resint7_9$I2, digits=1, format="f")), "%, ",
                    tau^2, " = ", .(formatC(resint7_9$tau2, digits=2, format="f")),";",
                    H^2, " = ", .(formatC(resint7_9$H2, digits=2, format="f")))))}

forest(resint7_9,  showweights=TRUE,
       mlab=mlabfun7_9("RE Model", resint7_9),xlim=c(-40,30),cex=1.2, cex.lab=1, 
       ylim=c(-1, 7),
       psize=1, header="Cohort study (N= Attended ECEC, %)", slab=c('ALSPAC (N=4053, 61.24% )', 'GenR (N=788, 31.82%) ','DNBC (N=22 769, 51.92%)', 'EDEN (N=441, 50.86%)'))

### add column headings to the plot
text(15,6,cex=1.2,font=2, c("Weights (%)"))


