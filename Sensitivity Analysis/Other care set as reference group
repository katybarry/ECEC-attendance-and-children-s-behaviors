## sensitivity analysis other care set as the reference group

#load libraries
library(DSMolgenisArmadillo)
library(dsBaseClient)
library(dsHelper)
library(DSI)
library(DSOpal)

#login to all cohorts

tokenelfe <- armadillo.get_token("https://armadillo.sicopre.elfe-france.fr")
tokeneden <- armadillo.get_token("https://armadillo.sicopre.elfe-france.fr")
tokenalspac <- armadillo.get_token("https://alspac-armadillo.molgenis.org")
#### login

builder <- DSI::newDSLoginBuilder()
#ALSPAC
builder$append(
  server = "alspac",
  url = "https://alspac-armadillo.molgenis.org",
  token = tokenalspac,
  driver = "ArmadilloDriver")

#GEN R
builder$append(server = "generationR", 
               url = "https://opal.erasmusmc.nl", user = "", 
               password = "",
               driver = "OpalDriver")

# DNBC
builder$append(server = "dnbc",
               url = "https://opal.sund.ku.dk",
               user ="",
               password= "",
               driver = "OpalDriver" )
#EDEN
builder$append(server ="eden",
               url="https://armadillo.sicopre.elfe-france.fr",
               token=tokeneden,
               driver="ArmadilloDriver")
# INMA
builder$append(server = "INMA", 
               url = "https://opal.isglobal.org/repo", 
               user = "", 
               password = "",
               driver = "OpalDriver",
               profile='rock-inma')
# ELFE
builder$append(server ="elfe",
               url="https://armadillo.sicopre.elfe-france.fr",
               token=tokenelfe,
               driver="ArmadilloDriver")

# NINFEA
builder$append(server = "ninfea", 
               url = "https://www.lifecycle.unito.it", 
               user = "", 
               password = "",
               driver = "OpalDriver")
# all login
logindata <- builder$build()


###################### INTERNALIZING BEHAVIOR ##############################################
############################################################################################
#### we can see from this which cohorts are in which age brackets in internalizing behavior
c.data <- DSI::datashield.login(logins = logindata, restore="strata_other")

## internalizing behavior changing reference group
ds.class('int_data$f.type_care.5_7')
ds.class('int_data$f.type_care.7_10')
ds.class('int_data$f.type_care.10_14')

ds.changeRefGroup(
  x = 'int_data$f.type_care.5_7',
  ref = '2',
  newobj = 'f.ref_other.5_7',
  datasources=c.data[c('alspac','generationR','eden','elfe')])

ds.levels(x = "f.ref_other.5_7",
          datasources=c.data[c('alspac','generationR','eden','elfe')])

ds.changeRefGroup(
  x = 'int_data$f.type_care.7_10',
  ref = '2',
  newobj = 'f.ref_other.7_10',
  datasources=c.data[c('alspac','generationR','eden','dnbc')])

ds.levels(x = "f.ref_other.7_10",
          datasources=c.data[c('alspac','generationR','eden','dnbc')])

ds.changeRefGroup(
  x = 'int_data$f.type_care.10_14',
  ref = '2',
  newobj = 'f.ref_other.10_14',
  datasources=c.data[c('alspac','dnbc')])

ds.levels(x = "f.ref_other.10_14",
          datasources=c.data[c('alspac','dnbc')])


ds.dataFrame(x=c('int_data','f.ref_other.5_7'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="int_data", datasources = c.data[c('elfe')])
ds.dataFrame(x=c('int_data','f.ref_other.5_7','f.ref_other.7_10'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="int_data", datasources = c.data[c('eden')])
ds.dataFrame(x=c('int_data','f.ref_other.5_7','f.ref_other.7_10', 'f.ref_other.10_14'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="int_data", datasources = c.data[c('alspac')])
ds.dataFrame(x=c('int_data','f.ref_other.7_10', 'f.ref_other.10_14'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="int_data", datasources = c.data[c('dnbc')])
ds.dataFrame(x=c('int_data','f.ref_other.5_7','f.ref_other.7_10'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="int_data", datasources = c.data[c('generationR')])


