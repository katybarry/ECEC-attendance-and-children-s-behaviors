ds.dataFrame(x=c('ext_data','f.ref_other.5_7','f.ref_other.7_10'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="ext_data", datasources = c.data[c('eden')])
ds.dataFrame(x=c('ext_data','f.ref_other.5_7','f.ref_other.7_10', 'f.ref_other.10_14'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="ext_data", datasources = c.data[c('alspac')])
ds.dataFrame(x=c('ext_data','f.ref_other.7_10', 'f.ref_other.10_14'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="ext_data", datasources = c.data[c('dnbc')])
ds.dataFrame(x=c('ext_data','f.ref_other.5_7','f.ref_other.7_10'), check.names=TRUE, stringsAsFactors = FALSE, completeCases=FALSE, newobj="ext_data", datasources = c.data[c('generationR')])



datashield.workspace_save(conns = c.data, ws = "change_ref")







ds.colnames('int_data')
ds.dim('int_data')

#age bracket 5-6 years _______________________________________________________________________________________

############ sample size for each category ##################
ds.mean('int_data$n.int_pc_.5_7', type = 'split', datasources=c.data[c('alspac','generationR','eden','elfe', 'dnbc')])

ds.meanSdGp(x='int_data$n.int_pc_.5_7', y='int_data$f.ref_other.5_7', 
            type = 'split', datasources=c.data[c('alspac','generationR','eden','elfe','dnbc')])

ds.table('int_data$f.ref_other.5_7',exclude=NA)

# crude model by cohort age bracket 5-6 years (alspac, eden, elfe, genr)_______________________________________


#alspac


int_5_6_crude_alspac <- ds.glm(formula = "n.int_pc_.5_7 ~ f.ref_other.5_7",
                               data = "int_data",
                               family = "gaussian",
                               datasources = c.data[c('alspac')])
int_5_6_crude_alspac

dh.lmTab(
  model = int_5_6_crude_alspac, 
  type = "glm_ipd", 
  direction = "wide", 
  ci_format  = "paste")

#genr
int_5_6_crude_genr <- ds.glm(formula = "n.int_pc_.5_7 ~ f.ref_other.5_7",
                             data = "int_data",
                             family = "gaussian",
                             datasources = c.data[c('generationR')])

dh.lmTab(
  model = int_5_6_crude_genr, 
  type = "glm_ipd", 
  direction = "wide", 
  ci_format  = "paste")
int_5_6_crude_genr

#eden
int_5_6_crude_eden <- ds.glm(formula = "n.int_pc_.5_7 ~ f.ref_other.5_7",
                             data = "int_data",
                             family = "gaussian",
                             datasources = c.data[c('eden')])

dh.lmTab(
  model = int_5_6_crude_eden, 
  type = "glm_ipd", 
  direction = "wide", 
  ci_format  = "paste")
int_5_6_crude_eden

#elfe

int_5_6_crude_elfe <- ds.glm(formula = "n.int_pc_.5_7 ~ f.ref_other.5_7",
                             data = "int_data",
                             family = "gaussian",
                             datasources = c.data[c('elfe')])

int_5_6_crude_elfe

dh.lmTab(
  model = int_5_6_crude_elfe, 
  type = "glm_ipd", 
  direction = "wide", 
  ci_format  = "paste")


#crude forest plot meta-analysis for age bracket 5-6 years ________________________


yint5_6 <-c((int_5_6_crude_alspac$coefficients["f.ref_other.5_71", "Estimate"]),
            (int_5_6_crude_genr$coefficients["f.ref_other.5_71", "Estimate"]),
            (int_5_6_crude_eden$coefficients["f.ref_other.5_71", "Estimate"]),
            (int_5_6_crude_elfe$coefficients["f.ref_other.5_71", "Estimate"]))

yint5_6

seint5_6 <-c((int_5_6_crude_alspac$coefficients["f.ref_other.5_71", "Std. Error"]), 
             (int_5_6_crude_genr$coefficients["f.ref_other.5_71", "Std. Error"]),
             (int_5_6_crude_eden$coefficients["f.ref_other.5_71", "Std. Error"]),
             (int_5_6_crude_elfe$coefficients["f.ref_other.5_71", "Std. Error"]))

resint5_6 <- metafor::rma(yi=yint5_6, sei=seint5_6)
resint5_6


resint5_6$se


library('metafor')

mlabfun5_6 <- function(text, resint5_6) {
  list(bquote(paste(.(text),
                    ": Q = ", .(formatC(resint5_6$QE, digits=2, format="f")),
                    ", df = ", .(resint5_6$k - resint5_6$p),
                    ", p ", .(metafor:::.pval(resint5_6$QEp, digits=2, showeq=TRUE, sep=" ")), "; ",
                    I^2, " = ", .(formatC(resint5_6$I2, digits=1, format="f")), "%, ",
                    tau^2, " = ", .(formatC(resint5_6$tau2, digits=2, format="f")),";",
                    H^2, " = ", .(formatC(resint5_6$H2, digits=2, format="f")))))}

ds.table('int_data$f.ref_other.5_7', exclude=NA, datasources=c.data[c('alspac', 'generationR','eden','elfe')])

forest(resint5_6,  showweights=TRUE,
       mlab=mlabfun5_6("RE Model", resint5_6),xlim=c(-40,30),cex=1.2, cex.lab=1, 
       ylim=c(-1, 7),
       psize=1, header="Cohort study (N= Attended ECEC, %)", slab=c('ALSPAC (N=114, 6.61%)', 'GenR (N=926, 64.48% )','EDEN (N=262, 35.36%)','ELFE (N=3297, 30.41%)'))

### add column headings to the plot
text(15,6,cex=1.2,font=2, c("Weights (%)"))




# adjusted forest plot meta-analysis for age bracket 5-6 years______________________________________

#alspac
ds.colnames('int_data')

int_5_6_adj_alspac <- ds.glm(formula = "n.int_pc_.5_7 ~ f.ref_other.5_7+ n.birth_weight.5_7+ n.agebirth_m_y.5_7+ f.fam_splitup.0.5_7+ f.only_child.5_7+ f.sex.5_7+ f.edu_m_.0.5_7+ f.mom.emp.dich.5_7",
                             data = "int_data",
                             family = "gaussian",
                             datasources = c.data[c('alspac')])
int_5_6_adj_alspac
dh.lmTab(
  model = int_5_6_adj_alspac, 
  type = "glm_ipd", 
  direction = "wide", 
  ci_format  = "paste")

#genr
