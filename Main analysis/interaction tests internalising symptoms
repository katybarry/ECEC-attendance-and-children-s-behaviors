####################################################################################################
# meta-analysis 5 to 6 years ppd interaction model
####################################################################################################

# centre-based childcare-------------------------------
yint5_6 <-c((u_pooled_alspac56$pooled_mean[3]),
            (u_pooled_genr56$pooled_mean[3]),
            (u_pooled_eden56$pooled_mean[3]),
            (u_pooled_elfe56$pooled_mean[3]))

yint5_6

seint5_6 <-c((u_pooled_alspac56$pooled_se[3]),
             (u_pooled_genr56$pooled_se[3]),
             (u_pooled_eden56$pooled_se[3]),
             (u_pooled_elfe56$pooled_se[3]))


resint5_6 <- metafor::rma(yint5_6, sei=seint5_6)
resint5_6


# pooled results
resint5_6 #-3.8537 p=0.6587

# Informal childcare-------------------------------

yint5_6_other <-c((u_pooled_alspac56$pooled_mean[5]),
            (u_pooled_genr56$pooled_mean[5]),
            (u_pooled_eden56$pooled_mean[5]),
            (u_pooled_elfe56$pooled_mean[5]))

yint5_6_other

seint5_6_other <-c((u_pooled_alspac56$pooled_se[5]),
             (u_pooled_genr56$pooled_se[5]),
             (u_pooled_eden56$pooled_se[5]),
             (u_pooled_elfe56$pooled_se[5]))


resint5_6_other <- metafor::rma(yint5_6_other, sei=seint5_6_other)
resint5_6_other #0.7404

# pooled results
resint5_6_other #-2.0811 p=0.7404



